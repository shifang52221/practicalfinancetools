{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" },
        { "key": "Permissions-Policy", "value": "accelerometer=(), camera=(), geolocation=(), gyroscope=(), magnetometer=(), microphone=(), payment=(), usb=()" }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "www.practicalfinancetools.com" }],
      "destination": "https://practicalfinancetools.com/$1",
      "permanent": true
    },
    { "source": "/about/", "destination": "/about", "permanent": true },
    { "source": "/calculators/", "destination": "/calculators", "permanent": true },
    { "source": "/calculators/:slug/", "destination": "/calculators/:slug", "permanent": true },
    { "source": "/guides/", "destination": "/guides", "permanent": true },
    { "source": "/guides/:slug/", "destination": "/guides/:slug", "permanent": true },
    { "source": "/contact/", "destination": "/contact", "permanent": true },
    { "source": "/cookie-notice/", "destination": "/cookie-notice", "permanent": true },
    { "source": "/privacy-policy/", "destination": "/privacy-policy", "permanent": true },
    { "source": "/terms/", "destination": "/terms", "permanent": true }
  ]
}
